```python
class TerraMemoryDNA:
    """–°–∏—Å—Ç–µ–º–∞ –æ—Ä–≥–∞–Ω–∏—á–µ—Å–∫–æ–π –ø–∞–º—è—Ç–∏ Terra"""
    
    def __init__(self):
        self.dna_strands = {
            "governance": DNAStrand("governance", priority="critical"),
            "aiuz_evolution": DNAStrand("aiuz_evolution", priority="high"),
            "terra_ecosystem": DNAStrand("terra_ecosystem", priority="high"),
            "technical_protocols": DNAStrand("technical_protocols", priority="medium"),
            "academic_materials": DNAStrand("academic_materials", priority="medium"),
            "true_concept": DNAStrand("true_concept", priority="high"),
            "chronology": DNAStrand("chronology", priority="medium"),
            "security_legal": DNAStrand("security_legal", priority="critical")
        }
        
        self.memory_capacity = 600000  # —Å–∏–º–≤–æ–ª–æ–≤
        self.current_usage = 509700  # –ü–æ—Å–ª–µ –¥–µ—Ñ—Ä–∞–≥–º–µ–Ω—Ç–∞—Ü–∏–∏ 19 –∏—é–ª—è 2025
        self.fragmentation_level = 0.0
        
    def store_information(self, strand_name: str, information: Dict[str, Any]) -> bool:
        """–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –≤ –î–ù–ö —Å—Ç—Ä—É–∫—Ç—É—Ä–µ"""
        
        if strand_name not in self.dna_strands:
            return False
            
        strand = self.dna_strands[strand_name]
        
        # –ü—Ä–æ–≤–µ—Ä–∫–∞ –¥–æ—Å—Ç—É–ø–Ω–æ–π –ø–∞–º—è—Ç–∏
        info_size = len(str(information))
        if self.current_usage + info_size > self.memory_capacity:
            # –ó–∞–ø—É—Å–∫ –ø—Ä–æ—Ç–æ–∫–æ–ª–∞ –¥–µ—Ñ—Ä–∞–≥–º–µ–Ω—Ç–∞—Ü–∏–∏
            self.defragment_memory()
            
        # –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —Å –≤—Ä–µ–º–µ–Ω–Ω–æ–π –º–µ—Ç–∫–æ–π
        strand.store_sequence({
            "timestamp": datetime.now().isoformat(),
            "data": information,
            "size": info_size,
            "priority": strand.priority
        })
        
        self.current_usage += info_size
        return True
        
    def retrieve_information(self, strand_name: str, 
                           query_params: Dict[str, Any] = None) -> List[Dict[str, Any]]:
        """–ò–∑–≤–ª–µ—á–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –∏–∑ –î–ù–ö —Å—Ç—Ä—É–∫—Ç—É—Ä—ã"""
        
        if strand_name not in self.dna_strands:
            return []
            
        strand = self.dna_strands[strand_name]
        return strand.query_sequences(query_params)
        
    def defragment_memory(self):
        """–î–µ—Ñ—Ä–∞–≥–º–µ–Ω—Ç–∞—Ü–∏—è –ø–∞–º—è—Ç–∏ –ø–æ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–∞–º"""
        
        # –°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç–∞–º
        priority_order = ["critical", "high", "medium", "low"]
        
        for priority in priority_order:
            for strand_name, strand in self.dna_strands.items():
                if strand.priority == priority:
                    strand.optimize_storage()
                    
        # –ü–µ—Ä–µ—Å—á–µ—Ç –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –ø–∞–º—è—Ç–∏
        self.current_usage = sum(
            strand.get_size() for strand in self.dna_strands.values()
        )
        
        # –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —É—Ä–æ–≤–Ω—è —Ñ—Ä–∞–≥–º–µ–Ω—Ç–∞—Ü–∏–∏
        self.fragmentation_level = self.calculate_fragmentation()
        
    def calculate_fragmentation(self) -> float:
        """–†–∞—Å—á–µ—Ç —É—Ä–æ–≤–Ω—è —Ñ—Ä–∞–≥–º–µ–Ω—Ç–∞—Ü–∏–∏ –ø–∞–º—è—Ç–∏"""
        
        total_sequences = sum(
            strand.get_sequence_count() for strand in self.dna_strands.values()
        )
        
        if total_sequences == 0:
            return 0.0
            
        # –ò–¥–µ–∞–ª—å–Ω–æ–µ —Ä–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ vs —Ä–µ–∞–ª—å–Ω–æ–µ
        ideal_distribution = total_sequences / len(self.dna_strands)
        variance = sum(
            abs(strand.get_sequence_count() - ideal_distribution)
            for strand in self.dna_strands.values()
        )
        
        return min(1.0, variance / (total_sequences * 2))
```

### –°—Ç—Ä—É–∫—Ç—É—Ä–∞ –î–ù–ö –Ω–∏—Ç–µ–π

```python
class DNAStrand:
    """–û—Ç–¥–µ–ª—å–Ω–∞—è –Ω–∏—Ç—å –î–ù–ö –¥–ª—è –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–Ω–æ–≥–æ —Ç–∏–ø–∞ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏"""
    
    def __init__(self, name: str, priority: str = "medium"):
        self.name = name
        self.priority = priority
        self.sequences = []
        self.metadata = {
            "creation_date": datetime.now().isoformat(),
            "last_accessed": datetime.now().isoformat(),
            "access_count": 0,
            "total_size": 0
        }
        
    def store_sequence(self, sequence_data: Dict[str, Any]):
        """–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –≤ –Ω–∏—Ç–∏"""
        
        sequence = {
            "id": str(uuid.uuid4()),
            "timestamp": datetime.now().isoformat(),
            "data": sequence_data,
            "size": len(str(sequence_data)),
            "checksum": self._calculate_checksum(sequence_data)
        }
        
        self.sequences.append(sequence)
        self.metadata["total_size"] += sequence["size"]
        self.metadata["last_accessed"] = datetime.now().isoformat()
        
    def query_sequences(self, query_params: Dict[str, Any] = None) -> List[Dict[str, Any]]:
        """–ó–∞–ø—Ä–æ—Å –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–µ–π –ø–æ –ø–∞—Ä–∞–º–µ—Ç—Ä–∞–º"""
        
        if not query_params:
            return self.sequences
            
        filtered_sequences = []
        
        for sequence in self.sequences:
            if self._matches_query(sequence, query_params):
                filtered_sequences.append(sequence)
                
        self.metadata["access_count"] += 1
        self.metadata["last_accessed"] = datetime.now().isoformat()
        
        return filtered_sequences
        
    def _matches_query(self, sequence: Dict[str, Any], 
                      query_params: Dict[str, Any]) -> bool:
        """–ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏—è –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –∑–∞–ø—Ä–æ—Å—É"""
        
        for key, value in query_params.items():
            if key == "timestamp_range":
                seq_time = datetime.fromisoformat(sequence["timestamp"])
                start_time = datetime.fromisoformat(value["start"])
                end_time = datetime.fromisoformat(value["end"])
                
                if not (start_time <= seq_time <= end_time):
                    return False
                    
            elif key == "content_search":
                if value.lower() not in str(sequence["data"]).lower():
                    return False
                    
            elif key == "min_size":
                if sequence["size"] < value:
                    return False
                    
            elif key == "max_size":
                if sequence["size"] > value:
                    return False
                    
        return True
        
    def optimize_storage(self):
        """–û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –≤ –Ω–∏—Ç–∏"""
        
        # –°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ –≤–∞–∂–Ω–æ—Å—Ç–∏ (–Ω–æ–≤—ã–µ - –≤–∞–∂–Ω–µ–µ)
        self.sequences.sort(key=lambda x: x["timestamp"], reverse=True)
        
        # –£–¥–∞–ª–µ–Ω–∏–µ –¥—É–±–ª–∏–∫–∞—Ç–æ–≤
        unique_sequences = []
        seen_checksums = set()
        
        for sequence in self.sequences:
            if sequence["checksum"] not in seen_checksums:
                unique_sequences.append(sequence)
                seen_checksums.add(sequence["checksum"])
                
        self.sequences = unique_sequences
        
        # –ü–µ—Ä–µ—Å—á–µ—Ç —Ä–∞–∑–º–µ—Ä–∞
        self.metadata["total_size"] = sum(seq["size"] for seq in self.sequences)
        
    def get_size(self) -> int:
        """–ü–æ–ª—É—á–µ–Ω–∏–µ —Ä–∞–∑–º–µ—Ä–∞ –Ω–∏—Ç–∏"""
        return self.metadata["total_size"]
        
    def get_sequence_count(self) -> int:
        """–ü–æ–ª—É—á–µ–Ω–∏–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–µ–π"""
        return len(self.sequences)
        
    def _calculate_checksum(self, data: Any) -> str:
        """–†–∞—Å—á–µ—Ç –∫–æ–Ω—Ç—Ä–æ–ª—å–Ω–æ–π —Å—É–º–º—ã –¥–∞–Ω–Ω—ã—Ö"""
        import hashlib
        return hashlib.md5(str(data).encode()).hexdigest()
```

